{
	"name": "PWA By Babel",
	"short_name": "PWA Babel",
    "description": "WebBoard is your go to application for quick doodles, notes, or sketches!",
	"theme_color": "#E89642",
	"background_color": "#ffffff",
	"display": "standalone",
	"orientation": "porttrait",
	"scope": "./",
	"start_url":"./",
	"icons":[
		{
			"src": "./images/icono.png",
			"type": "image/png",
			"sizes": "512x512"
		}
		],
	"file_handlers": [
	    {      
	        "action": "/",                
                "accept":{
	              "application/pdf": [".pdf"],
	              "application/zip": [".zip"],
	              "application/vnd.etsi.asic-e+zip": [".xsig"] 
	      	}
	    }
	  ]
	
}
